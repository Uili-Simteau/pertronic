{
  "name": "pertonic-express-pg-react",
  "version": "1.0.0",
  "description": "Bare Bones express app with react view library",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test/index.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Uili-Simteau/pertronic"
  },
  "author": "Uili-Simteau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Uili-Simteau/pertronic/issues"
  },
  "homepage": "https://github.com/Uili-Simteau/pertronic",
  "dependencies": {
    "babel": "^6.5.2",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "pg": "*",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "babel-cli": "^6.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}